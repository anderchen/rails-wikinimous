<div class="wrapper">

  <div class="container articles">
    <% @articles.each do |article| %>
      <div class="articles">
        <h4><%= link_to article.title, article_path(article), class: "link-article"%></h4>
        <p><%= article.content %></p>
      </div>
      <div class="break">
        <br>
      </div>
    <% end %>
  </div>
</div>
